{
    "name": "propel/propel",
    "type": "library",
    "description": "Propel2 is an open-source Object-Relational Mapping (ORM) for PHP 7.0 and up.",
    "keywords": [
        "ORM",
        "persistence",
        "Active Record"
    ],
    "homepage": "http://www.propelorm.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "William Durand",
            "email": "william.durand1@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/yaml": "^3.4||^4.3||^5.0",
        "symfony/console": "^3.4||^4.3||^5.0",
        "symfony/finder": "^3.4||^4.3||^5.0",
        "symfony/validator": "^3.4||^4.3||^5.0",
        "symfony/filesystem": "^3.4||^4.3||^5.0",
        "symfony/config": "^3.4||^4.3",
        "symfony/translation": "^3.4||^4.3",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "monolog/monolog": "^1.3",
        "phpunit/phpunit": "^6.1.0"
    },
    "suggest": {
        "monolog/monolog": "The recommended logging library to use with Propel."
    },
    "autoload": {
        "psr-0": {
            "Propel": "src/"
        }
    },
    "bin": [
        "bin/propel"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
